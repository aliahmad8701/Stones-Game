<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="630.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="app.controller.LeaderboardController">
    <children>
        <Label layoutX="251.0" layoutY="40.0" text="Leaderboard" textAlignment="CENTER">
            <font>
                <Font name="System Bold" size="18.0"/>
            </font>
        </Label>
        <TableView fx:id="tblLeaderboard" layoutX="19.0" layoutY="97.0" prefHeight="246.0" prefWidth="593.0">
            <columns>
                <TableColumn fx:id="colDate" prefWidth="165.0" text="Date"/>
                <TableColumn fx:id="colPlayer1" prefWidth="107.0" text="Player 1"/>
                <TableColumn fx:id="colPlayer2" prefWidth="101.0" text="Player 2"/>
                <TableColumn fx:id="colWinner" prefWidth="127.0" text="Winner"/>
                <TableColumn fx:id="colTurns" prefWidth="92.0" text="Turns"/>
            </columns>
        </TableView>
        <Button layoutX="37.0" layoutY="38.0" mnemonicParsing="false" onAction="#onMainMenuPressed" text="&lt; Back"/>
    </children>
</AnchorPane>
